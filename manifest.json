{
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "version": "0.0.0",
    "default_locale": "en",
    "description": "__MSG_appDescription__",
    "author": "June07 <667@june07.com> (https://june07.com/)",
    "commands": {
        "open-devtools": {
            "suggested_key": {
                "default": "Alt +I",
                "mac": "Alt+I"
            },
            "description": "__MSG_keyCtrlI__"
        }
    },
    "devtools_page": "devtools.html",
    "action": {
        "default_icon": {
            "16": "./icon/icon16.png",
            "162": "./icon/icon16@2x.png",
            "163": "./icon/icon16@3x.png",
            "32": "./icon/icon32.png",
            "322": "./icon/icon32@2x.png",
            "323": "./icon/icon32@3x.png",
            "48": "./icon/icon48.png",
            "482": "./icon/icon48@2x.png",
            "483": "./icon/icon48@3x.png",
            "128": "./icon/icon128.png",
            "1282": "./icon/icon128@2x.png",
            "1283": "./icon/icon128@3x.png"
        },
        "default_title": "__MSG_appName__",
        "default_popup": "/dist/index.html"
    },
    "icons": {
        "16": "./icon/icon16.png",
        "162": "./icon/icon16@2x.png",
        "163": "./icon/icon16@3x.png",
        "32": "./icon/icon32.png",
        "322": "./icon/icon32@2x.png",
        "323": "./icon/icon32@3x.png",
        "48": "./icon/icon48.png",
        "482": "./icon/icon48@2x.png",
        "483": "./icon/icon48@3x.png",
        "128": "./icon/icon128.png",
        "1282": "./icon/icon128@2x.png",
        "1283": "./icon/icon128@3x.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "/dist/index.html#options",
    "permissions": [
        "storage",
        "background",
        "tabs",
        "identity",
        "identity.email",
        "notifications",
        "contextMenus",
        "debugger",
        "scripting"
    ],
    "host_permissions": ["<all_urls>"],
    "externally_connectable": {
        "matches": ["*://*.brakecode.com/*"]
    },
    "short_name": "__MSG_appShortName__",
    "version_name": "0.0.0"
}
